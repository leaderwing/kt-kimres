<?xml version="1.0" encoding="utf-8" ?>
<TransformRules>
	<!-- quantitativeAdjective -->

	<!-- ORIGINAL -->

	<!-- orginal plan -->
	<rule name="quantitativeAdjective" priority="1">
		<premiselist>
			<premise>
				<subject var="c" className="Plan" />
				<adjective var="d" value="original" />
			</premise>
		</premiselist>
		<consequentlist>
			<consequent>
				<subject var="c" className="OriginalPlan" />
				<relation var="b" value="subClassOf" />
				<object var="a" className="Plan" quantifier="*" />
			</consequent>
		</consequentlist>
	</rule>
	<!-- ====================================================== -->


	<!-- TOP-SELLING -->

	<!-- top-selling fragrance -->
	<rule name="superlativeAdjective" priority="1">
